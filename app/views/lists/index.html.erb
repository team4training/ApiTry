<p id="notice"><%= notice %></p>

<h1>Lists</h1>

<table>
  <thead>
    <tr>
      <th>Body</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @lists.each do |list| %>
      <tr>
        <td><%= list.influencer.name %></td>
        <td><%= list.body %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'All Influencers', influencers_path %>
